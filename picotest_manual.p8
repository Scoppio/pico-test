 cls()
 local a=(yscroll*linelen)+1
 local b=(a+pagelen)-1
 local body=sub(docs,a,b)
 local progress=(yscroll/yscrollmax)*(127-24)
 print(doctitle, 0, 0)
 print(body, 0, 12)
 rectfill(126,progress+20,127,progress+12)
end

